{
  "tags": {
    "allowUnknownTags": true
  },
  "plugins": [
    "plugins/markdown",
    "node_modules/jsdoc-vuejs"
  ],
  "source": {
    "includePattern": "\\.(vue|js)$"
  },
  "docdash": {
    "static": true,
    "sort": true,
    "sectionOrder": [
      "Classes",
      "Modules",
      "Externals",
      "Events",
      "Namespaces",
      "Mixins",
      "Tutorials",
      "Interfaces"
    ],
    "meta": {
      "title": "VuePleasure",
      "description": "",
      "keyword": ""
    },
    "search": true,
    "collapse": true,
    "wrap": false,
    "typedefs": true,
    "navLevel": 0,
    "private": true,
    "removeQuotes": "trim",
    "menu": {
      "Project Website": {
        "href": "https://pleasure.js.org/vue",
        "class": "menu-item",
        "id": "website_link"
      }
    }
  }
}
